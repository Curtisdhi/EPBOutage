<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="A virtual youtube subscription manager">
        <meta name="author" content="Hyperion Studios">
        <link rel="icon" href="{{ asset('favicon.ico') }}">

        <title>{% block title %}EPBOutage{% endblock %}</title>

        <!-- Bootstrap core CSS -->
        <link href="{{ asset('dist/css/vendor.min.css') }}" rel="stylesheet">
        <link href="{{ asset('dist/css/core.min.css') }}" rel="stylesheet">
        {% block stylesheets %}{% endblock %}

    </head>

    <body>
        {% block body %}{% endblock %}

        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
        <script src="{{ asset('dist/js/vendor.min.js') }}"></script>
        <script src="{{ asset('dist/js/core.min.js') }}"></script>

        {% block javascript %}{% endblock %}

    </body>
</html>
